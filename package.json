{
  "name": "biolak",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build --turbo",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "gen:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "gen:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "gen:db-schema": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix && prettier --write .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "pg:dev": "docker compose up -d postgres-dev"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.35.0",
    "@payloadcms/db-postgres": "3.35.0",
    "@payloadcms/live-preview-react": "3.35.0",
    "@payloadcms/next": "3.35.0",
    "@payloadcms/payload-cloud": "3.35.0",
    "@payloadcms/plugin-form-builder": "3.35.0",
    "@payloadcms/plugin-nested-docs": "3.35.0",
    "@payloadcms/plugin-redirects": "3.35.0",
    "@payloadcms/plugin-search": "3.35.0",
    "@payloadcms/plugin-seo": "3.35.0",
    "@payloadcms/richtext-lexical": "3.35.0",
    "@payloadcms/ui": "3.35.0",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "babel-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.3.1",
    "graphql": "^16.10.0",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "next-sitemap": "^4.2.3",
    "payload": "3.35.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.55.0",
    "sharp": "0.34.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.14.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.25.0",
    "eslint-config-next": "15.3.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
